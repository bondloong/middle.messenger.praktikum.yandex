{
  "name": "yandex.chat",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": "v18.16.1",
    "npm": "v9.8.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "tsc && vite build && node server.js",
    "format": "prettier --write \"src/**/*.{js,ts,css,md,json,hbs}\" --config ./.prettierrc",
    "eslint": "eslint \"src/**/*.{js,ts,json}\" --fix",
    "stylelint": "stylelint \"**/*.{css, less}\" --fix --allow-empty-input",
    "lint": "eslint . && stylelint '**/*.{css,less}' && tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "less": "^4.2.0",
    "sass": "^1.71.1",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended-less": "^3.0.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-less": "^3.0.1",
    "stylelint-less": "^3.0.1",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "dependencies": {
    "@types/handlebars": "^4.1.0",
    "express": "^4.18.2",
    "handlebars": "^4.7.8",
    "path": "^0.12.7"
  }
}
